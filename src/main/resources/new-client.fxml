                  </rowConstraints>
                  <children>
                      <Label text="Full Name" />
                      <Label text="Address" GridPane.rowIndex="1" />
                      <TextField GridPane.columnIndex="1" />
                      <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" />
                      <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="90.0" text="Record" textAlignment="RIGHT" GridPane.columnIndex="1" GridPane.columnSpan="2" GridPane.rowIndex="2" />
