<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity"
      maxWidth="-Infinity"
      minHeight="-Infinity"
      minWidth="-Infinity"
      prefHeight="400.0"
      prefWidth="600.0"
      stylesheets="@style.css"
      xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="gui.controller.MenuController">
   <children>
      <HBox alignment="CENTER_RIGHT"
            prefHeight="50.0"
            prefWidth="200.0"
            styleClass="HBox"
            VBox.vgrow="NEVER">
         <children>
            <Button fx:id="btnMenuBar"
                    alignment="CENTER"
                    mnemonicParsing="false"
                    onAction="#loadPane"
                    text="Show Form 1"
                    textAlignment="RIGHT"
                    HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin></Button>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <SplitPane dividerPositions="0.29797979797979796"
                 focusTraversable="true"
                 prefHeight="160.0"
                 prefWidth="200.0"
                 VBox.vgrow="ALWAYS">
         <items>
            <AnchorPane maxWidth="200.0"
                        minHeight="0.0"
                        minWidth="0.0"
                        prefWidth="150.0">
               <children>
                  <Accordion layoutX="-42.0"
                             layoutY="96.0"
                             maxWidth="-Infinity"
                             AnchorPane.bottomAnchor="5.0"
                             AnchorPane.leftAnchor="5.0"
                             AnchorPane.rightAnchor="5.0"
                             AnchorPane.topAnchor="5.0">
                     <panes>
                        <TitledPane animated="false"
                                    text="untitled 1">
                           <content>
                              <AnchorPane minHeight="0.0"
                                          minWidth="0.0"
                                          prefHeight="180.0"
                                          prefWidth="200.0">
                                 <children>
                                    <Button fx:id="openFrm2"
                                            mnemonicParsing="false"
                                            onAction="#loadPane2"
                                            text="open form 2"
                                            AnchorPane.leftAnchor="0.0"
                                            AnchorPane.rightAnchor="0.0"
                                            AnchorPane.topAnchor="0.0" />
                                    <Button fx:id="openFrm3"
                                            layoutX="40.0"
                                            layoutY="36.0"
                                            mnemonicParsing="false"
                                            onAction="#loadPane3"
                                            text="open form 3"
                                            AnchorPane.leftAnchor="0.0"
                                            AnchorPane.rightAnchor="0.0"
                                            AnchorPane.topAnchor="35.0" />
                                 </children></AnchorPane>
                           </content>
                        </TitledPane>
                        <TitledPane animated="false"
                                    text="untitled 2">
                           <content>
                              <AnchorPane minHeight="0.0"
                                          minWidth="0.0"
                                          prefHeight="180.0"
                                          prefWidth="200.0" />
                           </content>
                        </TitledPane>
                        <TitledPane animated="false"
                                    text="untitled 3">
                           <content>
                              <AnchorPane minHeight="0.0"
                                          minWidth="0.0"
                                          prefHeight="180.0"
                                          prefWidth="200.0" />
                           </content>
                        </TitledPane>
                     </panes>
                  </Accordion>
               </children>
            </AnchorPane>
            <AnchorPane minWidth="200.0">
               <children>
                  <VBox fx:id="dataPane"
                        layoutX="10.0"
                        layoutY="50.0"
                        AnchorPane.bottomAnchor="10.0"
                        AnchorPane.leftAnchor="10.0"
                        AnchorPane.rightAnchor="10.0"
                        AnchorPane.topAnchor="10.0">
                  </VBox>
               </children>
            </AnchorPane>
         </items>
      </SplitPane>
   </children>
</VBox>

